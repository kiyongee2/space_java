package classes.arrays;

public class ArrayTest {

	public static void main(String[] args) {
		// 1. 문자열 배열
		String[] cars = new String[3];
		
		for(int i = 0; i < cars.length; i++) {
			System.out.println(cars[i]);
		}
		System.out.println("------------------");
		
		//자동차 모델명 입력
		cars[0] = "Sonata";
		cars[1] = "Morning";
		
		for(int i = 0; i < cars.length; i++) {
			System.out.println(cars[i]);
		}
		
		

	}

}
