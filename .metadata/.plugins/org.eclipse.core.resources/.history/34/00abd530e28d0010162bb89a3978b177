package classes2.constant;

import java.util.Calendar;

//Week 열거형 상수 정의
enum Week{
	SUNDAY,
	MONDAY,
	TUESDAY,
	WEDNESDAY,
	THURSDAY,
	FRIDAY,
	SATUTDAY
}

public class WeekTest {

	public static void main(String[] args) {
		// 오늘 무슨 요일?
		Calendar cal = Calendar.getInstance();
		System.out.println(cal.getTime());

	}

}













