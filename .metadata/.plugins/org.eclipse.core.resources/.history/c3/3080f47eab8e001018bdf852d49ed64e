package classes2.drink;

//매출전표 테스트
public class SaleStatement {

	public static void main(String[] args) {
		// Drink 생성자 호출
		Drink coffee = new Drink("커피", 2500, 4);
		// Alcohol 인스턴스 생성
		Alcohol soju = new Alcohol("소주", 4000, 2);
		
		//출력
		Drink.printTitle();
		coffee.printData();
		soju.printData();

	}

}
