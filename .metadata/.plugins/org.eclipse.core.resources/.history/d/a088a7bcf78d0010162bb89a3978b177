package classes2.inheritance.car;

/*
    접근 제어자 - protected
    - 상속받는 클래스에서만 접근 가능 
 */
public class Car {
	protected String brand;  //브랜드명
	protected String model;  //모델명
	protected int speed;     //속도
	
	//생성자
	public Car(String brand, String model) {
		this.brand = brand;
		this.model = model;
		this.speed = 0;
	}
}
